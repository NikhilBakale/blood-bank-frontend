# Frontend Production Environment Variables
# Copy to .env.production and configure

# Backend API URL
VITE_API_URL=https://your-backend-api.com

# Azure OpenAI Configuration (Optional - for AI chatbot)
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
VITE_AZURE_OPENAI_KEY=your-azure-openai-key
VITE_AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini

# Google Gemini API (Optional - alternative to Azure OpenAI)
VITE_GEMINI_API_KEY=your-gemini-api-key

# Application Settings
VITE_APP_NAME=Blood Bank Buddy
VITE_APP_VERSION=1.0.0
